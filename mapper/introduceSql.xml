<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="introduceSql">
    <select id="interview">
        select 
            *
        from Portpolio.ContactMe
    </select>
    <select id="portfolio">
        select 
            id
            ,  name
            ,  datetime
            ,  position
            ,  ability
            ,  phone
            ,  email           
        from Portpolio.ContactMe
    </select>    
</mapper>

